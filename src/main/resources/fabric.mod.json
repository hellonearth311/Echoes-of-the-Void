{
	"schemaVersion": 1,
	"id": "echoesofthevoid",
	"version": "${version}",
	"name": "Echoes of the Void",
	"description": "A Minecraft mod that introduces rare structures, mysterious mobs, and powerful items, which are leaking through the fabric of reality. Explore the world like never before as traces of other dimensions show themselves everywhere.",
	"authors": [
		"Swarit Narang"
	],
	"contact": {
		"homepage": "https://github.com/hellonearth311/echoes-of-the-void",
		"sources": "https://github.com/hellonearth311/echoes-of-the-void"
	},
	"license": "CC0-1.0",
	"icon": "assets/echoesofthevoid/icon.png",
	"environment": "*",
	"entrypoints": {
		"main": [
			"net.hellonearth311.echoesofthevoid.EchoesOfTheVoid"
		],
		"client": [
			"net.hellonearth311.echoesofthevoid.EchoesOfTheVoidClient"
		]
	},
	"mixins": [
		"echoesofthevoid.mixins.json",
		{
			"config": "echoesofthevoid.client.mixins.json",
			"environment": "client"
		}
	],
	"depends": {
		"fabricloader": ">=0.16.14",
		"minecraft": "~1.21.8",
		"java": ">=21",
		"fabric-api": "*"
	},
	"suggests": {
		"modmenu": "*",
		"cloth-config": "*"
	}
}